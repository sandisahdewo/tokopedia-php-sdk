{
	"name": "sandi/tokopedia-php-sdk",
	"type": "library",
	"description": "PHP SDK for Tokopedia API.",
	"license": "BSD-3-Clause",
	"keywords": [
		"tokopedia",
		"tokopedia-api",
		"tokopedia-sdk",
		"tokopedia-sdk-api",
		"tokopedia-api-sdk"
	],
	"config": {
		"sort-packages": true
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.2-dev"
		}
	},
	"autoload": {
		"psr-4": {
			"Gandung\\Tokopedia\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Gandung\\Tokopedia\\Tests\\": "tests/"
		}
	},
	"require": {
		"php": "^7.1",
		"guzzlehttp/guzzle": "^7.0",
		"psr/cache": "^1.0",
		"psr/http-client": "^1.0",
		"psr/http-message": "^1.0"
	},
	"require-dev": {
		"cache/cache": "^1.0",
		"phpunit/phpunit": "^9.4@dev"
	}
}
